Mouse 1-1


df, bodyparts, scorer = dlc2kinematics.load_data("F:/DLC-analysis/test-7-sniffertotoesies/Attempt-7-boopsniffertotoesies-BR-2022-12-05/videos/8-29-22 1-1 SCI 8WDLC_resnet50_Attempt-7-boopsniffertotoesiesDec5shuffle1_250000.h5")



# Compute velocity

df_vel = dlc2kinematics.compute_velocity(df, bodyparts=['all'])

vel_file_path = f'C:/Users/14632/OneDrive - Indiana University/Desktop/DeepLabCut/x-correlation-re-re-analysis/analysis-attempt-8-1/DLC2Kinematics-1-4/DLC2K-1-5/velocity-1-1.csv'


df_vel.to_csv(vel_file_path)


# Compute acceleration

df_acc = dlc2kinematics.compute_acceleration(df, bodyparts=['all'])

acc_file_path = f'C:/Users/14632/OneDrive - Indiana University/Desktop/DeepLabCut/x-correlation-re-re-analysis/analysis-attempt-8-1/DLC2Kinematics-1-4/DLC2K-1-5/acceleration-1-1.csv'

df_acc.to_csv(acc_file_path)


# Compute speed

df_speed = dlc2kinematics.compute_speed(df, bodyparts=['all'])

speed_file_path = f'C:/Users/14632/OneDrive - Indiana University/Desktop/DeepLabCut/x-correlation-re-re-analysis/analysis-attempt-8-1/DLC2Kinematics-1-4/DLC2K-1-5/speed-1-1.csv'

df_speed.to_csv(speed_file_path)



# The following section for joints but i haven't gotten it to work yet


# Define the joints dictionary

joints_dict={}



# Map joint names to lists of body parts

joints_dict['N-MB-TB-j']  = ['Nose', 'Midbody', 'Tailbase']

joints_dict['LH-hip-j']  = ['Midbody', 'Tailbase', 'L.H. Knee']

joints_dict['LH-knee-j']  = ['Tailbase', 'L.H. Knee', 'L.H. Ankle']

joints_dict['LH-ankle-j']  = ['L.H. Knee', 'L.H. Ankle', 'L.H. metatarsals']

joints_dict['LH-d0-j']  = ['L.H. Ankle', 'L.H. metatarsals', 'L.H. Thumbnub']

joints_dict['LH-d1-j']  = ['L.H. Ankle', 'L.H. metatarsals', 'L.H. Medial digit']

joints_dict['LH-d2-j']  = ['L.H. Ankle', 'L.H. metatarsals', 'L.H. medial digit']

joints_dict['LH-d3-j']  = ['L.H. Ankle', 'L.H. metatarsals', 'L.H. distal digit']

joints_dict['LH-d4-j']  = ['L.H. Ankle', 'L.H. metatarsals', 'L.H. Distal digit']

joints_dict['RH-hip-j']  = ['Midbody', 'Tailbase', 'R.H. Knee']

joints_dict['RH-knee-j']  = ['Tailbase', 'R.H. Knee', 'R.H. Ankle']

joints_dict['RH-ankle-j']  = ['R.H. Knee', 'R.H. Ankle', 'R.H. metatarsals']

joints_dict['RH-d0-j']  = ['R.H. Ankle', 'R.H. metatarsals', 'R.H. Thumbnub']

joints_dict['RH-d1-j']  = ['R.H. Ankle', 'R.H. metatarsals', 'R.H. Medial digit']

joints_dict['RH-d2-j']  = ['R.H. Ankle', 'R.H. metatarsals', 'R.H. medial digit']

joints_dict['RH-d3-j']  = ['R.H. Ankle', 'R.H. metatarsals', 'R.H. distal digit']

joints_dict['RH-d4-j']  = ['R.H. Ankle', 'R.H. metatarsals', 'R.H. Distal digit']




# Compute joint angles using the provided DataFrame and joints dictionary

joint_angles = dlc2kinematics.compute_joint_angles(df, joints_dict)



# Define the file path for saving the joint angles as a CSV file

joint_angles_file_path = r'C:/Users/14632/OneDrive - Indiana University/Desktop/DeepLabCut/x-correlation-re-re-analysis/analysis-attempt-8-1/DLC2Kinematics-1-4/DLC2K-1-5/joint_angles-1-1.csv'



# Save the joint angles DataFrame to a CSV file

joint_angles.to_csv(joint_angles_file_path)


