acc_file_path = f'C:/Users/14632/OneDrive - Indiana University/Desktop/DeepLabCut/x-correlation-re-re-analysis/analysis-attempt-8-1/DLC2Kinematics-1-4/DLC2K-1-5/acceleration-9-4.csv'

df_acc.to_csv(acc_file_path)


# Compute speed

df_speed = dlc2kinematics.compute_speed(df, bodyparts=['all'])

speed_file_path = f'C:/Users/14632/OneDrive - Indiana University/Desktop/DeepLabCut/x-correlation-re-re-analysis/analysis-attempt-8-1/DLC2Kinematics-1-4/DLC2K-1-5/speed-9-4.csv'

df_speed.to_csv(speed_file_path)



